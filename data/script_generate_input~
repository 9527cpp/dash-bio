#!/bin/bash

echo "Script Usage: script_generate_input <pdb_file_name>"

pdb_file=$1

#Create input data file
python pdbParse.py $pdb_file > input_data1.js

#Create input stick style file
python pdbStyles.py $pdb_file > input_style_stick.js

#Create input style file
python pdbStyles1.py $pdb_file > input_style_cartoon.js

#create sphere style file
cat input_style1.js | sed 's/cartoon/sphere/g' > input_style_sphere.js
