{
  "name": "dash_bio",
  "version": "0.0.1",
  "description": "Dash wrapper around CircosJS",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "build:js-dev": "webpack --mode development",
    "build:js": "webpack --mode production",
    "build:py": "node ./extract-meta src/lib/components > dash_bio/metadata.json && copyfiles package.json dash_bio",
    "build:all": "npm run build:js & npm run build:py",
    "build:all-dev": "npm run build:js-dev & npm run build:py"
  },
  "author": "Matthew Chan",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "15.4.2",
    "react-circos": "^1.0.0",
    "react-dom": "15.4.2"
  },
  "devDependencies": {
    "argos-cli": "^0.0.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "finalhandler": "^1.1.1",
    "npm": "^6.1.0",
    "puppeteer": "^1.6.0",
    "react-docgen": "^2.20.1",
    "serve-static": "^1.13.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.2"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
